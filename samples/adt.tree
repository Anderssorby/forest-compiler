data Maybe a
  = Just a
  | Nothing

data Either a b
  = Left a
  | Right b

data Expression
  = Identifier Ident
  | Number Int
  | Infix OperatorExpr Expression Expression

foo :: Int -> Maybe Int
foo n =
  case n of
    5 -> Just 10
    n -> Nothing

main :: Maybe Int
main = foo 5

data NumberList
  = Node Int NumberList
  | Tail Int

foo :: NumberList
foo = Node 10 (Tail 5)

